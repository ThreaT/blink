            <Property name="text" type="java.lang.String" value="package com.yourapp.scenario.home.read;&#xa;&#xa;import com.blink.Feature;&#xa;import com.blink.Intention;&#xa;import com.blink.Protocol;&#xa;import com.blink.Report;&#xa;import com.blink.Scenario;&#xa;import com.blink.WebServer;&#xa;import java.io.IOException;&#xa;import java.util.logging.Level;&#xa;import java.util.logging.Logger;&#xa;import javax.servlet.http.HttpServlet;&#xa;import javax.servlet.http.HttpServletRequest;&#xa;import javax.servlet.http.HttpServletResponse;&#xa;import javax.websocket.Session;&#xa;&#xa;public class Home extends HttpServlet implements Feature {&#xa;&#xa;    private static final Intention intention = new Intention(Protocol.HTTP, &quot;&quot;, &quot;&quot;, &quot;/home&quot;, true);&#xa;    public static final Scenario scenario = new Scenario(Home.class, intention);&#xa;&#xa;    @Override&#xa;    public Boolean fit(Session session, String message, HttpServletRequest request, HttpServletResponse response) {&#xa;        Logger.getLogger(Home.class.getName()).log(Level.INFO, &quot;Running Home: fit(Session session, String message, HttpServletRequest request, HttpServletResponse response)&quot;);&#xa;        return true;&#xa;    }&#xa;&#xa;    @Override&#xa;    public void main(Session session, String message, HttpServletRequest request, HttpServletResponse response) {&#xa;        try {&#xa;            Logger.getLogger(Home.class.getName()).log(Level.INFO, &quot;Running Home: main(Session session, String message, HttpServletRequest request, HttpServletResponse response)&quot;);&#xa;            response.getWriter().println(template(session, message, request, response));&#xa;        } catch (IOException ex) {&#xa;            Logger.getLogger(Home.class.getName()).log(Level.SEVERE, null, ex);&#xa;        }&#xa;    }&#xa;&#xa;    /**&#xa;     * Acceptance requirements:&#xa;     *&#xa;     * &lt;ol&gt;&#xa;     * &lt;li&gt;&#xa;     * Ensure that processing completes in under 10 ms.&#xa;     * &lt;/li&gt;&#xa;     * &lt;/ol&gt;&#xa;     *&#xa;     * @param session Session&#xa;     * @param message String&#xa;     * @param request HttpServletRequest&#xa;     * @param response HttpServletResponse&#xa;     * @return Report&#xa;     */&#xa;    @Override&#xa;    public Report test(Session session, String message, HttpServletRequest request, HttpServletResponse response) {&#xa;        Logger.getLogger(Home.class.getName()).log(Level.INFO, &quot;Running Home: test(Session session, String message, HttpServletRequest request, HttpServletResponse response)&quot;);&#xa;        Report report = new Report(1, 1, &quot;100%&quot;, &quot;&quot;);&#xa;        Long start = System.currentTimeMillis();&#xa;        main(session, message, request, response);&#xa;        Long end = System.currentTimeMillis();&#xa;        if (end - start &gt; 10) {&#xa;            report.setSuccessful(report.getSuccessful() - 1);&#xa;        }&#xa;        report.setPercentage(report.calculatePercentage(report.getTotal(), report.getSuccessful()));&#xa;        return report;&#xa;    }&#xa;&#xa;    public static String template(Session session, String message, HttpServletRequest request, HttpServletResponse response) {&#xa;        &#xa;    }&#xa;}&#xa;"/>
